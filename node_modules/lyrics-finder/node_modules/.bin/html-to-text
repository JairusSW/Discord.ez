#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/mnt/c/Users/jairu/.pnpm-store/html-to-text@5.1.1/node_modules/html-to-text/bin/cli.js/node_modules:/mnt/c/Users/jairu/.pnpm-store/html-to-text@5.1.1/node_modules/html-to-text/bin/node_modules:/mnt/c/Users/jairu/.pnpm-store/html-to-text@5.1.1/node_modules/html-to-text/node_modules:/mnt/c/Users/jairu/.pnpm-store/html-to-text@5.1.1/node_modules:/mnt/c/Users/jairu/.pnpm-store/node_modules:/mnt/c/Users/jairu/node_modules:/mnt/c/Users/node_modules:/mnt/c/node_modules:/mnt/c/Users/jairu/.pnpm-store/lyrics-finder@21.7.0/node_modules/lyrics-finder/node_modules:/mnt/c/Users/jairu/.pnpm-store/lyrics-finder@21.7.0/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../html-to-text@5.1.1/node_modules/html-to-text/bin/cli.js" "$@"
else
  exec node  "$basedir/../../../../../html-to-text@5.1.1/node_modules/html-to-text/bin/cli.js" "$@"
fi
