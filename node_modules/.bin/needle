#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/mnt/c/Users/jairu/.pnpm-store/needle@2.6.0/node_modules/needle/bin/node_modules:/mnt/c/Users/jairu/.pnpm-store/needle@2.6.0/node_modules/needle/node_modules:/mnt/c/Users/jairu/.pnpm-store/needle@2.6.0/node_modules:/mnt/c/Users/jairu/.pnpm-store/node_modules:/mnt/c/Users/jairu/node_modules:/mnt/c/Users/node_modules:/mnt/c/node_modules:/mnt/c/Users/jairu/Documents/Projects/Discord.ez/node_modules/needle/bin/node_modules:/mnt/c/Users/jairu/Documents/Projects/Discord.ez/node_modules/needle/node_modules:/mnt/c/Users/jairu/Documents/Projects/Discord.ez/node_modules:/mnt/c/Users/jairu/Documents/Projects/node_modules:/mnt/c/Users/jairu/Documents/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../needle/bin/needle" "$@"
else
  exec node  "$basedir/../needle/bin/needle" "$@"
fi
